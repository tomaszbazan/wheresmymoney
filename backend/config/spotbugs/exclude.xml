<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="~.*\.generated\..*"/>
    </Match>

    <Match>
        <Class name="~.*\$.*Builder"/>
    </Match>

    <!-- Return value ignored in test methods -->
    <Match>
        <Class name="~.*Test"/>
        <Method name="~.*shouldThrowException.*"/>
        <Bug pattern="RV_RETURN_VALUE_IGNORED_INFERRED"/>
    </Match>
</FindBugsFilter>